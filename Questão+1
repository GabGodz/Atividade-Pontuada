
programa {

	funcao inicio()
	{
		// Entrada dos valores A, B e C
		escreva("Informe o valor de A: ")
		real A
		leia(A)

		escreva("Informe o valor de B: ")
		real B
		leia(B)

		escreva("Informe o valor de C: ")
		real C
		leia(C)

		// Verificação e exibição do resultado
		se (A + B < C) {
			escreva("A + B é menor que C.")
		} senao {
			escreva("A + B é maior que C.")
		}
	}
}
